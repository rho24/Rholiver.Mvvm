<UserControl x:Class="Rholiver.Mvvm.Examples.Navigation.Views.ShellView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="400" d:DesignWidth="500">
    
  <StackPanel Height="400" Width="500" Background="LightGray">
    <Button Name="OpenSimpleDialog" Content="Open Simple Dialog" FontSize="20" />
    <StackPanel Orientation="Horizontal">
      <Button Name="OpenReturningDialog" Content="Open Returning Dialog" FontSize="20" />
      <TextBlock Name="DialogReturnResult" FontSize="24" Margin="20,0,0,0" VerticalAlignment="Center" />
    </StackPanel>
  </StackPanel>
</UserControl>
